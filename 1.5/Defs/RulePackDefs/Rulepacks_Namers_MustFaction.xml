    <RulePackDef>
        <defName>Must_NamerPerson_Male</defName>
        <rulePack>
            <rulesStrings>
                <li>r_name(p=6)->[Must_Names_Male] [Must_Surnames]</li>
                <li>r_name(p=3)->[Must_Names_Male] '[Must_Nicknames]' [Must_Surnames]</li>
            </rulesStrings>
            <rulesRaw>
                <li Class="Rule_File">
                    <keyword>Must_Names_Male</keyword>
                    <path>NameBanks/Must_Names_Male</path>
                </li>
                <li Class="Rule_File">
                    <keyword>Must_Surnames</keyword>
