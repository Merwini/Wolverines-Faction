<Defs>

	<!--Ferret-->
	<BodyDef>
		<defName>REP_BW_Ferret_Body</defName>
		<label>ferret drone</label>
		<corePart>
			<def>REP_BW_DroneBody</def>
			<height>Middle</height>
			<depth>Outside</depth>
            <groups>
                <li>CoveredByNaturalArmor</li>
            </groups>
			<parts>
				<!--Head Section-->
				<li>
					<def>REP_BW_DroneNeck</def>
					<coverage>0.20</coverage>
					<height>Top</height>
					<depth>Inside</depth>
					<parts>
						<li>
							<def>REP_BW_DroneHead</def>
							<coverage>0.5</coverage>
							<parts>
								<li>
									<def>REP_BW_DroneBrain</def>
									<coverage>0.25</coverage>
								</li>
							</parts>
						</li>
					</parts>
				</li>
				<!--Face Section-->
				<li>
					<def>REP_BW_DroneFace</def>
					<coverage>0.05</coverage>
					<parts>
						<li>
							<def>REP_BW_DroneEye</def>
							<coverage>0.25</coverage>
						</li>
						<li>
							<def>REP_BW_DroneEar</def>
							<coverage>0.25</coverage>
						</li>
					</parts>
				</li>
				<!--Internals Section-->
				<li>
					<def>REP_BW_DroneHeart</def>
					<coverage>0.1</coverage>
					<depth>Inside</depth>
				</li>
				<li>
					<def>REP_BW_DroneLiver</def>
					<coverage>0.1</coverage>
					<depth>Inside</depth>
				</li>
				<!--Wheels Section-->
				<li>
					<def>REP_BW_Ferret_Leg</def>
					<customLabel>left front wheel</customLabel>
					<coverage>0.0825</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>REP_BW_Ferret_Foot</def>
							<coverage>0.5</coverage>
							<customLabel>left front tire</customLabel>
						</li>
					</parts>
				</li>
				<li>
					<def>REP_BW_Ferret_Leg</def>
					<customLabel>right front wheel</customLabel>
					<coverage>0.0825</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>REP_BW_Ferret_Foot</def>
							<coverage>0.5</coverage>
							<customLabel>right front tire</customLabel>
						</li>
					</parts>
				</li>
				<li>
					<def>REP_BW_Ferret_Leg</def>
					<customLabel>left rear wheel</customLabel>
					<coverage>0.0825</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>REP_BW_Ferret_Foot</def>
							<coverage>0.5</coverage>
							<customLabel>left rear tire</customLabel>
						</li>
					</parts>
				</li>
				<li>
					<def>REP_BW_Ferret_Leg</def>
					<customLabel>right rear wheel</customLabel>
					<coverage>0.0825</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>REP_BW_Ferret_Foot</def>
							<coverage>0.5</coverage>
							<customLabel>right rear tire</customLabel>
						</li>
					</parts>
				</li>
				<!--Weapon Section-->
				<li>
					<def>REP_BW_Ferret_Tool</def>
					<coverage>0.1</coverage>
					<groups>
						<li>HeadAttackTool</li>
					</groups>
				</li>
			</parts>
		</corePart>
	</BodyDef>

