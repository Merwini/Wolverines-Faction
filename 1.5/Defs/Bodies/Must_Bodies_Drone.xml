<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!--RC Car-->
	<BodyDef>
		<defName>Must_RC_Body</defName>
		<label>RC drone</label>
		<corePart>
			<def>Must_DroneBody</def>
			<height>Middle</height>
			<depth>Outside</depth>
			<parts>
				<!--Head Section-->
				<li>
					<def>Must_DroneNeck</def>
					<coverage>0.10</coverage>
					<height>Top</height>
					<depth>Inside</depth>
					<parts>
						<li>
							<def>Must_DroneHead</def>
							<coverage>0.5</coverage>
							<parts>
								<li>
									<def>Must_DroneBrain</def>
									<coverage>0.25</coverage>
								</li>
							</parts>
						</li>
					</parts>
				</li>
				<!--Face Section-->
				<li>
					<def>Must_DroneFace</def>
					<coverage>0.05</coverage>
					<parts>
						<li>
							<def>Must_DroneEye</def>
							<coverage>0.25</coverage>
						</li>
						<li>
							<def>Must_DroneEar</def>
							<coverage>0.25</coverage>
						</li>
					</parts>
				</li>
				<!--Internals Section-->
				<li>
					<def>Must_DroneHeart</def>
					<coverage>0.1</coverage>
					<depth>Inside</depth>
				</li>
				<li>
					<def>Must_DroneLiver</def>
					<coverage>0.1</coverage>
					<depth>Inside</depth>
				</li>
				<!--Wheels Section-->
				<li>
					<def>Must_RC_Leg</def>
					<customLabel>front left wheel</customLabel>
					<coverage>0.0825</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>Must_RC_Foot</def>
							<coverage>0.5</coverage>
							<customLabel>front left tire</customLabel>
						</li>
					</parts>
				</li>
				<li>
					<def>Must_RC_Leg</def>
					<customLabel>front right wheel</customLabel>
					<coverage>0.0825</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>Must_RC_Foot</def>
							<coverage>0.5</coverage>
							<customLabel>front right tire</customLabel>
						</li>
					</parts>
				</li>
				<li>
					<def>Must_RC_Leg</def>
					<customLabel>rear left wheel</customLabel>
					<coverage>0.0825</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>Must_RC_Foot</def>
							<coverage>0.5</coverage>
							<customLabel>rear left tire</customLabel>
						</li>
					</parts>
				</li>
				<li>
					<def>Must_RC_Leg</def>
					<customLabel>rear right wheel</customLabel>
					<coverage>0.0825</coverage>
					<height>Bottom</height>
					<parts>
						<li>
							<def>Must_RC_Foot</def>
							<coverage>0.5</coverage>
							<customLabel>rear right tire</customLabel>
						</li>
					</parts>
				</li>
				<!--Weapon Section-->
				<li>
					<def>Must_RC_Tool</def>
					<coverage>0.1</coverage>
					<groups>
						<li>HeadAttackTool</li>
					</groups>
				</li>
			</parts>
		</corePart>
	</BodyDef>
</Defs>